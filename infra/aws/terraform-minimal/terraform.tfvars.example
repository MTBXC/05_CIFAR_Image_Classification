# Example Terraform variables file for MLFlow deployment
# Copy this to terraform.tfvars and fill in your values

# AWS Configuration
aws_region = "eu-north-1"
environment = "dev"

# Network Configuration
vpc_cidr = "10.0.0.0/16"
public_subnet_cidr = "10.0.1.0/24"

# EC2 Configuration
ec2_instance_type = "t3.micro"     # 1 vCPU, 1GB RAM (Free Tier eligible)
ec2_volume_size = 20               # 20GB storage

# SSH Public Key (required)
# Generate with: ssh-keygen -t rsa -b 4096 -C "your_email@example.com"
# Then copy the public key content here
ssh_public_key = "ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAACAQ... your_email@example.com"